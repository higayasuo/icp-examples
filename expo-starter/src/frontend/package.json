    "expo-file-system": "~18.0.8",
    "expo-font": "~13.0.2",
    "expo-ii-integration": "^0.1.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-storage-universal": "^0.3.1",
    "expo-storage-universal-native": "^0.3.1",
    "expo-storage-universal-web": "^0.3.1",
    "expo-system-ui": "~4.0.8",
