  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~18.3.12",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.3.3",
    "vitest": "^2.1.8"
  },
  "private": true,
