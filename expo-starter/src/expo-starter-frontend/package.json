{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "expo-starter-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm install && npx expo install --fix",
    "start": "expo start --clear",
    "build": "npx expo export -p web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "dependencies": {
    "@dfinity/agent": "file:dfinity-agent-0.20.2.tgz",
    "@dfinity/auth-client": "^0.20.2",
    "@dfinity/bls-verify": "file:dfinity-bls-verify-0.20.2.tgz",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/identity": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "@expo/vector-icons": "^14.0.2",
    "@noble/curves": "file:noble-curves-1.2.0.tgz",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.14",
    "buffer": "^6.0.3",
    "expo": "^52.0.35",
    "expo-asset": "~11.0.3",
    "expo-file-system": "~18.0.8",
    "expo-font": "~13.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "ic-vetkd-utils": "file:ic-vetkd-utils-0.1.0.tgz",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "expo-crypto": "~14.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "jsdom": "^26.0.0",
    "typescript": "~5.3.3",
    "vitest": "^2.1.8"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": []
      }
    }
  }
}
